@model lab1.Models.Manager.ManagerRegistrationModel
 
<h2>Регистрация</h2>
 
<form asp-action="ManagerRegistration" asp-controller="Manager" asp-anti-forgery="true">
    <div class="validation" asp-validation-summary="ModelOnly"></div>
    <div>
        <div class="input-group mb-3">
            <select class="custom-select" id="inputGroupSelect01" asp-for="@Model!.BankId">
                @foreach (var bank in Model.Banks)
                {
                    <option value="@bank.Id">@bank.LegalName</option>
                }
            </select>
        </div>
        <div>
            <input type="submit" value="Подтвердить"/>
        </div>
    </div>
</form>